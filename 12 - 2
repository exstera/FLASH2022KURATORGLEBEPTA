p1 = 'z' * 69
while 'zzz' in p1 or 'cx' in p1 or 'xx' in p1:
    p1 = p1.replace('zzz','zxxx')
    p1 = p1.replace('cx','z')
    p1 = p1.replace('xx','x')

p2 = 'z' * 432
while 'zzz' in p2 or 'cx' in p2 or 'xx' in p2:
    p2 = p2.replace('zzz','zxxx')
    p2 = p2.replace('cx','z')
    p2 = p2.replace('xx','x')
print(max(p2.count('z'),p1.count('z')) - min(p2.count('z'),p1.count('z')))



